            </div>
        `;
  }).join("");

  // Fetch and update images dynamically
  document.querySelectorAll(".fav-image").forEach(img => {
    let pageUrl = img.dataset.url;
    fetch(pageUrl)
      .then(response => response.text())
      .then(html => {
        let parser = new DOMParser();
        let doc = parser.parseFromString(html, "text/html");
        let ogImage = doc.querySelector('meta[property="og:image"]');

        if (ogImage && ogImage.content) {
          img.src = ogImage.content; // Use the Open Graph image
        }
      });
  });

