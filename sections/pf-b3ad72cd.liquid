  let favorites = JSON.parse(localStorage.getItem("favorites")) || [];

  if (!favContainer) return;

  // Set white text for the container
  favContainer.style.color = "white";

  if (favorites.length === 0) {
    favContainer.innerHTML = "<p>No favourites yet.</p>";
    return;
  }

  favContainer.innerHTML = favorites.map(fav => {
    return `
            <div class="favorite-item" style="margin-bottom:20px; padding:10px; border-bottom:1px solid white;">
